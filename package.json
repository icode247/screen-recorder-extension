    "keywords": [
      "chrome-extension",
      "screen-recording",
      "cursor-tracking",
      "video-recording"
    ],
    "author": "CursorFlow Team",
    "license": "MIT",
    "devDependencies": {
      "copy-webpack-plugin": "^11.0.0",
      "rimraf": "^5.0.1",
      "webpack": "^5.88.2",
      "webpack-cli": "^5.1.4"
    }
  }