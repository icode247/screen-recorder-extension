{
    "name": "cursorflow-extension",
    "version": "1.0.0",
    "description": "Intelligent screen recording Chrome extension with automatic cursor-following zoom effects",
    "main": "background/service-worker.js",
    "scripts": {
      "build": "webpack --mode=production",
      "dev": "webpack --mode=development --watch",
      "clean": "rimraf dist",
      "setup": "chmod +x setup.sh && ./setup.sh"
